public class sumOfElement {
    static void sumElement(int arr[]){
        int sum=0;
        for(int i=0;i<arr.length;i++){
            sum=arr[i]+sum;
        }
        System.out.println("sum of Element is : "+ sum);
    }
    public static void main(String[] args) {
        int arr[]={1,5,3};
        int sum=0;
        sumElement(arr);
        
    }
}
